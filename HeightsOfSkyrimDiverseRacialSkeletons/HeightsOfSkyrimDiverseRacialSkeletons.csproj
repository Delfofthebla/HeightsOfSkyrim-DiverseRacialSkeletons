    <WarningsAsErrors>CS8602;CS8604;CS8600;CS8601;CS8603;CS8610;CS8618;CS8619;CS8620;CS8625;CS8629;CS8714;</WarningsAsErrors>
  </PropertyGroup>

  <ItemGroup>
